[pull]
  rebase = true
[push]
  default = upstream
[merge]
  tool = vimdiff
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  edit = !subl $(git status --short | awk '$1 ~ /^M|A|U/ {print $2}' ) # !$EDITOR $(git status --short | awk '$1 ~ /^M|A|U/ {print $2}' )
  open = !$EDITOR $(git diff --name-only master..$(git rev-parse --abbrev-ref HEAD))
  ci = commit
  co = checkout
  st = status
  s  = status
  br = branch
  b  = branch
  d  = diff
  ds = diff --staged
  ln = log --name-status
  l  = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ci) %C(bold blue)<%an>%Creset' --abbrev-commit
  la = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ci) %C(bold blue)<%an>%Creset' --abbrev-commit --all --graph

  #Custom alias
  cit = commit -m "temp" --no-verify
  rbi = rebase -i HEAD~3
